{
    "name": "rshief/nab3a",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "bangpound/consul-bundle": "^0.1.0@dev",
        "clue/json-stream": "^0.1.0",
        "dlapps/consul-php-envvar": "^1.1",
        "guzzlehttp/oauth-subscriber": "^0.3.0",
        "mkraemer/react-pcntl": "^2.2",
        "php-amqplib/rabbitmq-bundle": "^1.13",
        "react/child-process": "dev-master",
        "react/eep": "dev-master",
        "symfony/console": "^3.3",
        "symfony/expression-language": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "symfony/debug-bundle": "^3.3",
        "symfony/dotenv": "^3.3",
        "symfony/phpunit-bridge": "^3.3"
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "php-cs-fixer": [
            "php-cs-fixer fix --rules=@PSR1,@PSR2,no_unused_imports -- src/",
            "php-cs-fixer fix --rules=@PSR1,@PSR2,no_unused_imports -- tests/"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "source"
        },
        "sort-packages": true
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BN4T6HMZMJK31JT7SWS8N7MK",
            "allow-contrib": "true"
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
